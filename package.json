{
  "name": "ts-template",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "======================start======================": "",
    "build": "pnpm clean && tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "start": "node -r tsconfig-paths/register dist/esm/src/index.js",
    "======================test======================": "",
    "lint": "eslint --quiet --fix \"{src,test}/**/*.ts\"",
    "lint:ci": "eslint --quiet \"{src,test}/**/*.ts\"",
    "test": "find ./src ./test -name '*.test.ts' | xargs tsx --test",
    "======================npm======================": "",
    "clean": "rimraf dist",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "src/**/*.ts",
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "!src/**/*_test.ts",
    "!dist/**/*_test.js",
    "!dist/**/*_test.js.map",
    "!dist/**/*_test.d.ts",
    "!dist/**/*_test.d.ts.map",
    "!src/**/*.{test,spec}.ts",
    "!dist/**/*.{test,spec}.js",
    "!dist/**/*.{test,spec}.js.map",
    "!dist/**/*.{test,spec}.d.ts",
    "!dist/**/*.{test,spec}.d.ts.map"
  ],
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/esm/src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/src/index.d.ts",
        "default": "./dist/esm/src/index.js"
      },
      "require": {
        "types": "./dist/cjs/src/index.d.ts",
        "default": "./dist/cjs/src/index.js"
      }
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/js": "^9.21.0",
    "@types/node": "^22.13.9",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "3.5.3",
    "rimraf": "^6.0.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.17.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "eslint --quiet --fix",
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "packageManager": "pnpm@10.6.1+sha512.40ee09af407fa9fbb5fbfb8e1cb40fbb74c0af0c3e10e9224d7b53c7658528615b2c92450e74cfad91e3a2dcafe3ce4050d80bda71d757756d2ce2b66213e9a3"
}
