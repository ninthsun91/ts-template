{
  "name": "ts-template",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "======================start======================": "",
    "build": "pnpm clean && pnpm build:esm && pnpm build:cjs",
    "start": "node -r tsconfig-paths/register dist/esm/src/index.js",
    "build:esm": "tsc -p tsconfig.json && echo '{\"type\":\"module\"}' > dist/esm/package.json",
    "build:cjs": "tsc -p tsconfig.cjs.json && echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "======================test======================": "",
    "lint": "eslint --quiet --fix \"{src,test}/**/*.ts\"",
    "lint:ci": "eslint --quiet \"{src,test}/**/*.ts\"",
    "test": "find ./src ./test -name '*.test.ts' | xargs tsx --test",
    "======================npm======================": "",
    "clean": "rimraf dist",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "src/**/*.ts",
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "!src/**/*_test.ts",
    "!dist/**/*_test.js",
    "!dist/**/*_test.js.map",
    "!dist/**/*_test.d.ts",
    "!dist/**/*_test.d.ts.map",
    "!src/**/*.{test,spec}.ts",
    "!dist/**/*.{test,spec}.js",
    "!dist/**/*.{test,spec}.js.map",
    "!dist/**/*.{test,spec}.d.ts",
    "!dist/**/*.{test,spec}.d.ts.map"
  ],
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/esm/src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/src/index.d.ts",
        "default": "./dist/esm/src/index.js"
      },
      "require": {
        "types": "./dist/cjs/src/index.d.ts",
        "default": "./dist/cjs/src/index.js"
      }
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.24.0",
    "@types/node": "^22.14.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "3.5.3",
    "rimraf": "^6.0.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.0"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "eslint --quiet --fix",
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "packageManager": "pnpm@10.7.1+sha512.2d92c86b7928dc8284f53494fb4201f983da65f0fb4f0d40baafa5cf628fa31dae3e5968f12466f17df7e97310e30f343a648baea1b9b350685dafafffdf5808"
}
