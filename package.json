{
  "name": "ts-template",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "======================start======================": "",
    "build": "tsc",
    "start": "node -r tsconfig-paths/register --env-file=.env dist/index.js",
    "dev": "tsx watch --env-file=.env src/index.ts",
    "======================test======================": "",
    "lint": "eslint --quiet --fix \"{src,test}/**/*.ts\"",
    "lint:ci": "eslint --quiet \"{src,test}/**/*.ts\"",
    "test": "find ./src ./test -name '*.test.ts' | xargs tsx --test --env-file=.env",
    "======================npm======================": "",
    "clean": "find . -name 'node_modules' -type d -prune -print -exec rm -rf '{}' \\;"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.3.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@10.5.2+sha512.da9dc28cd3ff40d0592188235ab25d3202add8a207afbedc682220e4a0029ffbff4562102b9e6e46b4e3f9e8bd53e6d05de48544b0c57d4b0179e22c76d1199b"
}
